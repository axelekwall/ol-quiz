{
  "name": "ol-quiz",
  "version": "1.0.0",
  "description": "OL Quiz web app",
  "main": "src/server",
  "engines": {
    "node": "8.6.0",
    "npm": "5.3.0"
  },
  "scripts": {
    "test": "test",
    "clean": "rm -rf $OUTPUT_DIR",
    "lint": "eslint --fix --ext js --ext jsx src",
    "prod-build-client": "webpack --config ./webpack/config.js",
    "prod-build-server": "babel ./src -d $OUTPUT_DIR/server --source-maps",
    "prod-build": "npm run clean && npm run prod-build-client && npm run prod-build-server",
    "postinstall": "if [ \"$NODE_ENV\" = production ] ; then npm run prod-build ; fi",
    "start": "node $OUTPUT_DIR/server/server",
    "predev-start": "npm run clean",
    "dev-start": "nodemon -e js,jsx,md,mustache -w ./src --exec ./dev-tools/babel-run -- ./src/server",
    "dev": "nf start -w --procfile ./dev-tools/Procfile.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axelekwall/ol-quiz.git"
  },
  "author": "Osqledaren dev team",
  "license": "MIT",
  "dependencies": {
    "aphrodite": "^1.1.0",
    "apollo-client": "^1.9.3",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^7.1.2",
    "babel-eslint": "^8.0.1",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-register": "^6.14.0",
    "body-parser": "^1.16.0",
    "express": "^4.14.0",
    "graphql": "^0.11.7",
    "graphql-server-express": "^1.1.3",
    "graphql-tag": "^2.4.2",
    "graphql-tools": "^2.2.1",
    "json-loader": "^0.5.4",
    "minilog": "^3.1.0",
    "nodemon": "^1.11.0",
    "react": "^16.0.0",
    "react-apollo": "^1.4.16",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.2",
    "react-router": "^4.2.0",
    "react-router-redux": "^4.0.7",
    "redux-thunk": "^2.2.0",
    "rollbar": "^2.2.9",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-manifest-plugin": "^1.0.1"
  },
  "devDependencies": {
    "eslint": "^4.8.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "foreman": "^2.0.0"
  }
}
